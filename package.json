{
  "name": "upload-artifact-compressible",
  "version": "1.0.0",
  "description": "Upload a build artifact that can be used by subsequent workflow steps - will compress also",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "release": "ncc build src/upload-artifact.ts && git add -f dist/",
    "check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:test\" \"npm:build\"",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "test": "jest --testTimeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/echapmanFromBunnings/upload-artifact-compressible.git"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Artifacts",
    "Upload"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/echapmanFromBunnings/upload-artifact-compressible/issues"
  },
  "homepage": "https://github.com/echapmanFromBunnings/upload-artifact-compressible#readme",
  "dependencies": {
    "@actions/artifact": "^1.1.2",
    "@actions/core": "^1.10.0",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.18.8",
    "@typescript-eslint/parser": "^5.48.0",
    "@vercel/ncc": "^0.36.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.2.0",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
